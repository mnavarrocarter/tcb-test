{
  "name": "engineered/coding_test_php_gacela",
  "description": "A PHP Coding Test using Gacela",
  "type": "project",
  "license": "GPL-3.0-or-later",
  "require": {
    "php": ">=7.4",
    "ext-pdo": "*",
    "ext-mysqli": "*",
    "ext-json": "*",
    "gacela-project/gacela": "dev-master",
    "symfony/http-client": "^5.3",
    "kriswallsmith/buzz": "^1.2",
    "nyholm/psr7": "^1.5"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "symfony/var-dumper": "^6.0",
    "friendsofphp/php-cs-fixer": "^3.9",
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "Engineered\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Engineered\\": "tests/"
    }
  }
}
